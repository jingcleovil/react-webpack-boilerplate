{
  "name": "task-app",
  "version": "1.0.0",
  "description": "Rudimentary Task-App that ncludes ImmutableJS, FixedDataTable, Flux and React",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "production": "npm run prodjs; npm run css-login; npm run css",
    "webpack": "webpack --config config/webpack.js",
    "prodjs": "webpack --config config/webpack.prod.js",
    "webpack-watch": "webpack --config config/webpack.js --watch --colors",
    "css": "cat node_modules/fixed-data-table/dist/fixed-data-table.min.css | cleancss -o src/client/css/style.min.css"
  },
  "dependencies": {
    "debug": "^2.1.3",
    "express": "^4.12.3",
    "fixed-data-table": "^0.1.2",
    "jquery": "^2.1.4",
    "object-assign": "^2.0.0",
    "react": "^0.13.3",
    "react-d3-components": "^0.4.8",
    "reqwest": "^1.1.5",
    "when": "^3.7.3"
  },
  "devDependencies": {
    "babel": "^5.0.12",
    "babel-core": "^5.0.12",
    "babel-loader": "^5.0.0",
    "clean-css": "^3.2.10",
    "flux": "^2.0.3",
    "immutable": "^3.7.2",
    "jest-cli": "^0.4.0",
    "lodash": "^3.8.0",
    "react-bootstrap": "^0.22.0",
    "react-router": "^0.13.3",
    "react-tools": "^0.13.3",
    "webpack": "^1.8.4"
  },
  "jest": {
    "scriptPreprocessor": "./tools/preprocessor.js",
    "unmockedModulePathPatterns": [
      "react"
    ],
    "testPathDirs": [
      "./src/app",
      "./src/client"
    ]
  }
}